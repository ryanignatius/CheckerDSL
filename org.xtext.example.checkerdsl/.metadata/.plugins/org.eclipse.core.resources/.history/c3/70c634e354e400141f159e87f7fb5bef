  private ArrayList test;
  
  public ArrayList getTest() {
    return this.test;
  }
  
  public void setTest(final ArrayList test) {
    this.test = test;
  }
  
  public void readTest() {
    test = new ArrayList();
    for (int i=0; i<n; i++){
    	ArrayList temp = new ArrayList();
    	for (int j=0; j<n; j++){
    		temp.add(intReader());
    	}
    	test.add(temp);
    }
  }
  
  public void readTest(final int idx1, final int idx2) {
    ArrayList atemp = test.get(idx1);
    atemp.set(idx2,intReader());
    test.set(idx1,atemp);
  }
  
  public void readInput() {
