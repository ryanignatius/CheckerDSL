  private ArrayList p;
  
  public ArrayList getP() {
    return this.p;
  }
  
  public void setP(final ArrayList p) {
    this.p = p;
  }
  
  public void readP() {
    p = new ArrayList();
    for (int i=0; i<n; i++){
    	p.add(intReader());
    }
  }
  
  public void readP(final int idx) {
    p.set(idx,intReader());
  }
  
